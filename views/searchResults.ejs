<%- include('navbar') %>
<div id="results">
    <h2>Search Results</h2>
    <% if (users.length > 0) { %>
        <ul>
            <% users.forEach(function(user) { %>
                <li>
                    <strong>Name:</strong> <%= user.name %> <br>
                    <strong>Email:</strong> <%= user.email %> <br>
                    <strong>Phone Number:</strong> <%= user.phone_number %> <br>
                </li>
            <% }) %>
        </ul>
    <% } else { %>
        <p>No users found.</p>
    <% } %>
    <a href="/search">Search Again</a>
</div>
